{
  "name": "quant_flat_ibm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "format:diff": "prettier --list-different \"src/**/*.{js,vue,scss}\"",
    "lint-fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test": "jest"
  },
  "main": "background.js",
  "dependencies": {
    "@fontsource/ibm-plex-sans": "^5.0.5",
    "@qiskit-community/qiskit-vue": "^3.4.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.5.0",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/vue3-jest": "^29.2.6",
    "axios": "^1.5.0",
    "browserify-fs": "^1.0.0",
    "core-js": "^3.8.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^11.1.1",
    "http-proxy-middleware": "^2.0.6",
    "http-server": "^14.1.1",
    "jest-environment-jsdom": "^29.7.0",
    "joi": "^17.9.2",
    "jsdom": "^22.1.0",
    "multer": "^1.4.5-lts.1",
    "nvm": "^0.0.4",
    "path-browserify": "^1.0.1",
    "preview": "^0.1.3",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "~9.0.6",
    "primevue": "^3.32.0",
    "tar": "^6.1.15",
    "unity-webgl": "^3.5.1",
    "vite": "^4.0.0",
    "vue": "^3.2.45",
    "vue-axios": "^3.5.2",
    "vue-demi": "^0.14.5",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^4.1.6",
    "vue-unity-webgl": "^1.2.0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0",
    "deasync": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@mdi/font": "^7.2.96",
    "@types/multer": "^1.4.7",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.8.1",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.25",
    "sass-loader": "^10.4.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.2",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-jest": "^3.0.7",
    "vue-style-loader": "^4.1.3"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/eslint-config-prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-deprecated-slot-attribute": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist_electron/",
      "<rootDir>/the-bloch-sphere/Library/PackageCache/",
      "<rootDir>/unity-vr/Library/PackageCache/"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)",
      "**/src/tests/**/?(*.)+(spec|test).js"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
